{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-alert": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "semi": true,
        "printWidth": 80,
        "tabWidth": 2,
        "endOfLine": "auto"
    }
    ]
  }
}
